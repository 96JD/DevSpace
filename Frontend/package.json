{
	"name": "dev-space",
	"description": "The frontend part of the web application 'Dev Space' that provides a platform for developers to interact with one another",
	"version": "1.0.0",
	"author": "Jacob Dolorzo",
	"license": "ISC",
	"packageManager": "pnpm@9.7.0+sha256.b35018fbfa8f583668b2649e407922a721355cd81f61beeb4ac1d4258e585559",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"postinstall": "cd .. && husky",
		"ng": "ng",
		"build": "ng build",
		"dev": "ng serve -o",
		"watch": "ng build --watch --configuration development",
		"lint": "ng lint",
		"odata:convert": "node odata2ts.js https://localhost:7177/odata/$metadata src/app/shared/odata-models.ts",
		"odata:prettier": "prettier --write --parser typescript src/app/shared/odata-models.ts",
		"odata:generate": "concurrently \"pnpm odata:convert\" \"pnpm odata:prettier\""
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	},
	"dependencies": {
		"96jd-odata-query-utils": "^18.0.2",
		"@angular/animations": "^18.1.4",
		"@angular/common": "^18.1.4",
		"@angular/compiler": "^18.1.4",
		"@angular/core": "^18.1.4",
		"@angular/forms": "^18.1.4",
		"@angular/platform-browser": "^18.1.4",
		"@angular/platform-browser-dynamic": "^18.1.4",
		"@angular/router": "^18.1.4",
		"@ng-icons/core": "^29.3.0",
		"@ng-icons/heroicons": "^29.3.0",
		"axios": "^1.7.3",
		"google-libphonenumber": "^3.2.38",
		"intl-tel-input": "^23.8.1",
		"ngx-bootstrap": "^18.0.2",
		"ngx-intl-tel-input": "^3.2.0",
		"rxjs": "^7.8.1",
		"tslib": "^2.6.3",
		"zone.js": "^0.14.10"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^18.1.4",
		"@angular/cli": "^18.1.4",
		"@angular/compiler-cli": "^18.1.4",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/forms": "^0.5.7",
		"@types/lodash": "^4.17.7",
		"@types/node": "^22.1.0",
		"angular-eslint": "18.2.0",
		"concurrently": "^8.2.2",
		"eslint": "^9.8.0",
		"husky": "^9.1.4",
		"lint-staged": "^15.2.8",
		"lodash": "^4.17.21",
		"ngx-toastr": "^19.0.0",
		"prettier": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"tailwindcss": "^3.4.9",
		"typescript": "^5.5.4",
		"typescript-eslint": "8.0.1",
		"xml2js": "^0.6.2"
	}
}
